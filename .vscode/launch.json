  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 5500,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "protocol": "inspector",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect=5500", "-r", "ts-node/register", "test"],
      "console": "integratedTerminal",
      "port": 5500,
      "cwd": "${workspaceFolder}",
      "localRoot": "${workspaceRoot}",
      "remoteRoot": "${workspaceRoot}"
    }
  ]
}
