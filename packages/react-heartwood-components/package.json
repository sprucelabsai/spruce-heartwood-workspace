{
    "name": "@sprucelabs/react-heartwood-components",
    "publishConfig": {
        "access": "public"
    },
    "version": "10.2.0",
    "main": "lib/index.js",
    "description": "React components for your Sprucebot Skill üí™üèº",
    "keywords": [
        "react",
        "node",
        "components",
        "sprucebot",
        "sprucelabs"
    ],
    "license": "SEE LICENCE in LICENCE",
    "homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
    "bugs": {
        "url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
    },
    "scripts": {
        "build": "npm run clean && npm run icons && npm run build:babel",
        "build:babel": "babel --root-mode upward --source-maps --extensions \".js\",\".tsx\",\".ts\" ./src --copy-files --no-copy-ignored --out-dir ./lib",
        "build:storybook": "npm run icons && build-storybook",
        "icons": "node ./generators/icons.js",
        "clean": "rm -rf lib/",
        "lint": "eslint --max-warnings=0 --ext=.js,.jsx,.ts,.tsx src && tsc -p . --noEmit",
        "lint:fix": "eslint --fix --max-warnings=0 --ext=.js,.jsx,.ts,.tsx src",
		"watch": "npm run build && chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'npm run compile_javascript'",
		"storybook": "npm run icons && start-storybook -p 6006 -s ./static"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0",
        "@sprucelabs/heartwood-components": "^10.2.0",
        "@sprucelabs/heartwood-skill": "^10.2.0",
        "classnames": "^2.2.5",
        "es6-tween": "^5.3.1",
        "exenv": "^1.2.2",
        "exif-orientation-image": "^1.0.1",
        "fontfaceobserver": "^2.1.0",
        "image-webpack-loader": "^4.3.1",
        "jsdom": "14.0.0",
        "jsdom-global": "3.0.2",
        "memoize-one": "^4.0.2",
        "moment": "^2.22.1",
        "moment-timezone": "^0.5.21",
        "react": "^16.13.1",
        "react-autosuggest": "^9.4.2",
        "react-avatar-editor": "^11.0.4",
        "react-dates": "^17.0.0",
        "react-dom": "^16.3.2",
        "react-dropzone": "^10",
        "react-image-crop": "3.0.11",
        "react-lottie": "^1.2.3",
        "react-markdown": "^4.1.0",
        "react-modal": "^3.6.1",
        "react-phone-number-input": "sprucelabsai/react-phone-number-input#2.4.7",
        "react-sortable-hoc": "^0.8.3",
        "react-table": "^6.8.6",
        "react-transition-group": "^2.5.2",
        "react-virtualized": "^9.21.0",
        "velocity-react": "^1.4.1",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "react-helmet": "^5.2.0",
        "@types/react-dom": "^16.8.2",
        "@types/react-table": "^6.8.4",
        "@types/fontfaceobserver": "^0.0.6",
        "@types/react": "^16.8.7",
        "@types/react-autosuggest": "^9.3.13",
        "@types/react-modal": "^3.10.5",
        "awesome-typescript-loader": "^5.2.1",
        "chokidar-cli": "^1.2.0",
        "faker": "^4.1.0",
        "husky": "^0.14.3",
        "jest": "^24.5.0",
        "prettier": "^1.19",
        "react-test-renderer": "^16.0.0",
        "sass-loader": "^8.0.2",
        "typescript": "^3.8",
        "uuid": "^3.3.2",
        "webpack": "^4.29.6"
    },
    "jest": {
        "roots": [
            "<rootDir>/src/"
        ],
        "coverageDirectory": "./coverage/",
        "collectCoverage": true,
        "coveragePathIgnorePatterns": [
            "<rootDir>/lib/",
            "<rootDir>/node_modules/"
        ]
    }
}
