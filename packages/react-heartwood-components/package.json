{
  "name": "@sprucelabs/react-heartwood-components",
  "publishConfig": {
    "access": "public"
  },
  "version": "10.2.0",
  "main": "lib/index.js",
  "description": "React components for your Sprucebot Skill üí™üèº",
  "keywords": [
    "react",
    "node",
    "components",
    "sprucebot",
    "sprucelabs"
  ],
  "license": "SEE LICENCE in LICENCE",
  "homepage": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit",
  "bugs": {
    "url": "https://github.com/sprucelabsai/workspace.sprucebot-skills-kit/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run icons && npm run build.babel",
    "build.babel": "babel --root-mode upward --source-maps --extensions \".js\",\".tsx\",\".ts\" ./src --copy-files --no-copy-ignored --out-dir ./lib",
    "build.storybook": "npm run icons && build-storybook",
    "icons": "node ./generators/icons.js",
    "clean": "rm -rf lib/",
    "lint": "eslint --max-warnings=0 --ext=.js,.jsx,.ts,.tsx src && tsc -p . --noEmit",
    "lint.fix": "eslint --fix --max-warnings=0 --ext=.js,.jsx,.ts,.tsx src",
    "watch": "npm run build && chokidar 'src/**/*.(js|jsx|ts|tsx)' -c 'npm run compile_javascript'",
    "storybook": "npm run icons && start-storybook -p 6006 -s ./static",
    "arkit": "arkit -f index.ts -o docs/images/arkit.svg",
    "arkit.png": "arkit -f index.ts -o docs/images/arkit.png"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@sprucelabs/heartwood-components": "^10.2.0",
    "@sprucelabs/heartwood-skill": "^10.2.0",
    "classnames": "^2.2.5",
    "es6-tween": "^5.3.1",
    "exenv": "^1.2.2",
    "exif-orientation-image": "^1.0.1",
    "fontfaceobserver": "^2.1.0",
    "image-webpack-loader": "^6.0.0",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "memoize-one": "^5.1.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-avatar-editor": "^11.0.9",
    "react-dates": "^21.8.0",
    "react-dom": "^16.3.2",
    "react-dropzone": "^11.0.1",
    "react-image-crop": "8.6.4",
    "react-lottie": "^1.2.3",
    "react-markdown": "^4.1.0",
    "react-modal": "^3.6.1",
    "react-phone-number-input": "sprucelabsai/react-phone-number-input#2.4.7",
    "react-sortable-hoc": "^1.11.0",
    "react-table": "^7.2.2",
    "react-transition-group": "^4.4.1",
    "react-virtualized": "^9.21.0",
    "velocity-react": "^1.4.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/react": "^16.9.41",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-table": "^7.0.19",
    "arkit": "^1.6.4",
    "awesome-typescript-loader": "^5.2.1",
    "chokidar-cli": "^2.1.0",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-helmet": "^6.1.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^9.0.2",
    "storybook-addon-jsx": "^7.3.0",
    "storybook-addon-react-docgen": "^1.2.41",
    "storybook-readme": "^5.0.8",
    "typescript": "^3.9.6",
    "uuid": "^8.2.0",
    "webpack": "^4.43.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ]
  }
}
